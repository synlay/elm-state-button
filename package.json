{
  "name": "elm-state-button",
  "version": "0.1.0",
  "license": "MIT",
  "description": "elm state button based on a small state machine",
  "scripts": {
    "build": "npm run make && npm run minify",
    "make": "elm-make ./src/Main.elm --output ./dist/main.js",
    "minify": "uglifyjs ./dist/main.js --output ./dist/main.min.js --source-map",
    "clean": "rimraf elm-stuff tests/elm-stuff node_modules",
    "test": "elm-test"
  },
  "devDependencies": {
    "elm-test": "0.18.3",
    "rimraf": "2.6.1"
  }
}
